@model IEnumerable<Millionaire.Domain.Entities.TotalUserResult>

@{
    ViewBag.Title = "ShowResult";
    Layout = "~/Views/Shared/_LayoutGamePage.cshtml";

}
<style>
    .body-content {
        color: whitesmoke;
    }
</style>
<h2>Результаты всех игр!</h2>


<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GameCount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TotalPrize)
        </th>
        
       
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.GameCount)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TotalPrize)
        </td>
        
    </tr>
}

</table>
